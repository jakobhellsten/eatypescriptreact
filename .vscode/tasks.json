{
    "version": "0.1.0",
    "command": "gulp",
    "isShellCommand": true,
    "args": [
        "--no-color"
    ],
    "tasks": [
		{
			"taskName": "bundle-read",
			"isBuildCommand": true,
			"showOutput": "always"
		},
		{
			"taskName": "bundle",
			"isBuildCommand": true,
			"showOutput": "always"
		},
        {
			"taskName": "compile",
			"isBuildCommand": true,
			"showOutput": "always"
		},
		{
			"taskName": "serve",
			"isBuildCommand": true,
			"showOutput": "always"
		},
		{
			"taskName": "watch",
			"isBuildCommand": true,
			"showOutput": "always"
		}
	]
}